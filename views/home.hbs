{{> head }}
{{> navbar }}
              
        <div class="jumbotron jumbotron-fluid">
            <div class="container">
                <h1 class="display-4">Bienvenido, {{nombre}}!</h1>
                <p class="lead">Sistema de gestión de creditos</p>
            </div>
        </div>
        <div class="container">
          <div class="row">
            <div class="col-10 offset-1">
                <div class="card-deck">
                    <div class="card">
                        <img class="card-img-top" src="assets/img/Tucuman.png" alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title">Ultimos cobros en Tucumán</h5>
                            <p class="card-text">Alvaro/Ramiro Morales -> $800</p>
